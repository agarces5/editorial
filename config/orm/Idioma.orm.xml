<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="App\Entity\Idioma" table="IDIOMA">
    <id name="idIdioma" type="integer" column="id_idioma">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idioma" type="string" column="idioma" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="iniciales" type="string" column="iniciales" length="2" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
